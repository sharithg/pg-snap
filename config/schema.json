{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "subset": {
      "type": "object",
      "properties": {
        "table": {
          "type": "string"
        },
        "schema": {
          "type": "string"
        },
        "where": {
          "type": "string"
        },
        "maxRowsPerTable": {
          "type": "number"
        }
      },
      "required": ["table", "schema", "where"]
    },
    "skipTables": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["subset", "skipTables"]
}
